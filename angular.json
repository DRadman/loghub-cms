              },
              {
                "input": "src/assets/theme/mdc-light-deeppurple.scss",
                "bundleName": "mdc-light-deeppurple",
                "inject": false
              },
              {
                "input": "src/assets/theme/mdc-light-indigo.scss",
                "bundleName": "mdc-light-indigo",
                "inject": false
              },
              {
                "input": "src/assets/theme/saga-blue.scss",
                "bundleName": "saga-blue",
