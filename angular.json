                "bundleName": "lara-dark-teal",
                "inject": false
              },
              {
                "input": "src/assets/theme/lara-dark-purple.scss",
                "bundleName": "lara-dark-purple",
                "inject": false
              },
              {
                "input": "src/assets/theme/arya-blue.scss",
                "bundleName": "arya-blue",
                "inject": false
              },
              {
                "input": "src/assets/theme/arya-green.scss",
                "bundleName": "arya-green",
                "inject": false
              },
              {
                "input": "src/assets/theme/arya-orange.scss",
                "bundleName": "arya-orange",
                "inject": false
              },
              {
                "input": "src/assets/theme/arya-purple.scss",
                "bundleName": "arya-purple",
                "inject": false
              },
              {
                "input": "src/assets/theme/bootstrap4-dark-blue.scss",
                "bundleName": "bootstrap4-dark-blue",
