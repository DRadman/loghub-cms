              },
              {
                "input": "src/assets/theme/bootstrap4-light-blue.scss",
                "bundleName": "bootstrap4-light-blue",
                "inject": false
              },
              {
                "input": "src/assets/theme/md-dark-indigo.scss",
                "bundleName": "md-dark-indigo",
