{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "npm",
    "analytics": "a3856c08-986e-4b43-be78-9da6e5a3ae28",
    "schematicCollections": [
      "@angular-eslint/schematics",
      "@schematics/angular"
    ]
  },
  "newProjectRoot": "projects",
  "projects": {
    "loghub-cms": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/loghub-cms",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss",
              {
                "input": "src/assets/theme/lara-light-blue.scss",
                "bundleName": "lara-light-blue",
                "inject": false
              },
              {
                "input": "src/assets/theme/lara-light-indigo.scss",
                "bundleName": "lara-light-indigo",
                "inject": false
              },
              {
